{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-js": "^8.11.3",
    "auth0-lock": "^10.24.1",
    "axios": "^0.17.1",
    "bootstrap": "4.0.0-beta.2",
    "excerpt-html": "^1.2.0",
    "format-currency": "^1.0.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "history": "^4.7.2",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "react-text-mask": "^5.0.2",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "semistandard \"src/**/*.js\" --fix --verbose | snazzy",
    "cosmos": "cosmos",
    "pretty": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.1.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "faker": "^4.1.0",
    "jest-enzyme": "^4.0.1",
    "jest-glamor-react": "^3.2.2",
    "jest-localstorage-mock": "^2.1.0",
    "prettier": "^1.9.2",
    "react-app-rewired": "^1.4.0",
    "react-cosmos": "^3.2.0",
    "react-cosmos-localstorage-proxy": "^3.1.1",
    "react-cosmos-redux-proxy": "^3.1.1",
    "react-cosmos-router-proxy": "^3.1.1",
    "react-cosmos-test": "^3.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.4.0",
    "rosie": "^1.6.0",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/node_modules/",
      "!src/*.{js}",
      "!src/cosmos.proxies.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "describe",
      "it",
      "xit",
      "expect",
      "FormData",
      "alert",
      "localStorage",
      "beforeEach"
    ]
  }
}
